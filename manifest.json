{
    "manifest_version": 2,
    "name": "Notes Downloader",
    "description": "This extension downloads notes.",
    "version": "1.0",
    "browser_action": {
        "default_icon": "images/icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab",
        "https://ajax.googleapis.com/"
    ],
    "background": {
        "page":"popup.html",
        "persistent": true
    }
}